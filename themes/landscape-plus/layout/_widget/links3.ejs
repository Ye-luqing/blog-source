<% if (site.posts.length){ %>
  <div class="widget-wrap">
    <h3 class="widget-title"><%= __('其它链接') %></h3>
    <div class="widget">
      <ul>
        <% for (var i in theme.links3){ %>
          <li>
            <a href="<%- theme.links3[i] %>" target="_blank"><%= i %></a>
          </li>
        <% } %>
      </ul>
    </div>
  </div>
<% } %>
